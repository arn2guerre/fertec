<style type="text/css">
.carousel{
    background: white;
    margin-top: 10px;
}
.carousel .item img{
    margin: left; /* Align slide image horizontally center */
}
.bs-example{
	margin: 20px;
}
img{
  float:left;
width: 425px;
height: 349px;
}
object{
  float :left;
}
</style>
<%= stylesheet_link_tag    "application", media: "all" %>
<%= javascript_include_tag "application" %>




